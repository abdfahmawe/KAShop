@section title {
    products
}
@section all {
    Show
}
@model List<Product>
<!DOCTYPE html>
<html>
<head>
    <title></title>
</head>
<body>

    <div class="card-body">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th style="width: 10px">#</th>
                    <th>Name</th>
                    <th>Action</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in @Model)
                {
                    <tr>
                        <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td><a class="btn btn-danger" href="/admin/products/dellete/@item.Id">Dellete</a>
                            <a class="btn btn-danger" href="/admin/products/edit/@item.Id">Edit</a>
                    </td>
                   
                </tr>}

            </tbody>
        </table>
    </div>
    </body>
    </html>